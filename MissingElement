import java.util.HashSet;
class Solution {
    public int solution(int[] A) {
        // write your code in Java SE 8

        int number = 1;
		HashSet<Integer> nums = new HashSet<Integer>();
          for(int i=0; i<A.length; i++)
                nums.add(A[i]);

		while (nums.contains(number)) {
			number++;
		}

		return number;
    }
}

